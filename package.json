{
  "name": "cncjs",
  "version": "1.5.0",
  "description": "A web-based interface for CNC milling controller running Grbl or TinyG2",
  "homepage": "https://github.com/cheton/cnc",
  "author": "Cheton Wu <cheton@gmail.com>",
  "main": "./dist/cnc/cnc.js",
  "bin": {
    "cnc": "./bin/cnc"
  },
  "files": [
    "bin",
    "dist",
    "static"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:cheton/cnc.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.x"
  },
  "preferGlobal": true,
  "scripts": {
    "package-update": "babel-node package-update",
    "prepublish": "npm run clean && npm run lint && npm run build",
    "prebuild": "npm run prebuild-prod",
    "prebuild-latest": "bash ./scripts/prebuild-latest.sh",
    "prebuild-dev": "bash ./scripts/prebuild-dev.sh",
    "prebuild-prod": "bash ./scripts/prebuild-prod.sh",
    "build": "gulp prod",
    "build-latest": "gulp prod",
    "build-dev": "gulp dev",
    "build-prod": "gulp prod",
    "clean": "bash -c 'rm -rf ./dist ./output'",
    "electron": "electron",
    "electron-builder": "build",
    "electron-packager": "electron-packager",
    "electron-rebuild": "electron-rebuild",
    "github-release": "babel-node github-release",
    "pack:darwin-x64": "bash -c 'scripts/electron-packager.sh --platform=darwin --arch=x64'",
    "pack:linux-ia32": "bash -c 'scripts/electron-packager.sh --platform=linux --arch=ia32'",
    "pack:linux-x64": "bash -c 'scripts/electron-packager.sh --platform=linux --arch=x64'",
    "pack:win32-ia32": "bash -c 'scripts/electron-packager.sh --platform=win32 --arch=ia32'",
    "pack:win32-x64": "bash -c 'scripts/electron-packager.sh --platform=win32 --arch=x64'",
    "build:mac-x64": "bash -c 'scripts/electron-builder.sh --mac --x64'",
    "build:linux-ia32": "bash -c 'scripts/electron-builder.sh --linux --ia32'",
    "build:linux-x64": "bash -c 'scripts/electron-builder.sh --linux --x64'",
    "build:win-ia32": "bash -c 'scripts/electron-builder.sh --win --ia32'",
    "build:win-x64": "bash -c 'scripts/electron-builder.sh --win --x64'",
    "start": "./bin/cnc",
    "start-electron": "electron ./dist/cnc/main",
    "predev": "bash ./watchpack-ignore-node-modules.sh",
    "dev": "npm run build-dev && NODE_ENV=development ./bin/cnc -vv",
    "prod": "npm run build-prod && NODE_ENV=production ./bin/cnc",
    "lint": "npm run eslint && npm run stylint",
    "eslint": "eslint *.js src bin gulp test",
    "stylint": "stylint src/web",
    "test": "node babel-tap --coverage test/*.js",
    "coveralls": "node babel-tap --coverage --coverage-report=text-lcov test/*.js | node_modules/.bin/coveralls"
  },
  "build": {
    "appId": "com.github.cnc",
    "app-category-type": "public.app-category.productivity",
    "asar": false,
    "mac": {
      "target": "default",
      "icon": "build/icon.icns"
    },
    "dmg": {
      "title": "cnc",
      "icon": "build/icon.icns",
      "background": "build/background.png",
      "icon-size": 80,
      "contents": [
        {
          "x": 448,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 344,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": "nsis",
      "iconUrl": "https://raw.githubusercontent.com/cheton/cnc/master/build/icon.ico",
      "loadingGif": "build/install-spinner.gif",
      "msi": false,
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "deb"
    }
  },
  "directories": {
    "buildResources": "build",
    "output": "output",
    "app": "dist/cnc"
  },
  "keywords": [
    "cnc",
    "gcode",
    "grbl",
    "arduino",
    "raspberry-pi",
    "react",
    "socket.io",
    "webappengine"
  ],
  "dependencies": {
    "async": "~2.0.0",
    "body-parser": "~1.15.1",
    "bootstrap": "~3.3.6",
    "chalk": "~1.1.3",
    "classnames": "~2.2.5",
    "colornames": "~1.1.1",
    "commander": "~2.9.0",
    "compression": "~1.6.2",
    "connect-multiparty": "~2.0.0",
    "connect-restreamer": "~1.0.3",
    "consolidate": "~0.14.1",
    "cookie-parser": "~1.4.3",
    "del": "~2.2.0",
    "delay": "~1.3.1",
    "errorhandler": "~1.4.3",
    "express": "~4.14.0",
    "express-session": "~1.14.0",
    "font-awesome": "~4.6.3",
    "fs-extra": "~0.30.0",
    "gcode-interpreter": "~0.9.3",
    "gcode-parser": "~0.8.2",
    "gcode-toolpath": "~0.6.2",
    "hogan.js": "~3.0.2",
    "i18next": "~3.4.0",
    "i18next-browser-languagedetector": "~0.3.0",
    "i18next-express-middleware": "~1.0.1",
    "i18next-node-fs-backend": "~0.1.2",
    "i18next-xhr-backend": "~0.6.0",
    "is-electron": "~2.0.0",
    "jsuri": "~1.3.1",
    "lodash": "~4.13.1",
    "method-override": "~2.3.6",
    "moment": "~2.14.1",
    "morgan": "~1.7.0",
    "mousetrap": "~1.6.0",
    "normalize.css": "~4.2.0",
    "pubsub-js": "~1.5.3",
    "range_check": "~1.2.0",
    "rc-slider": "~3.7.4",
    "rc-switch": "~1.4.2",
    "react": "~15.2.1",
    "react-addons-shallow-compare": "~15.2.1",
    "react-addons-update": "~15.2.1",
    "react-bootstrap": "~0.29.4",
    "react-css-modules": "~3.7.7",
    "react-dom": "~15.2.1",
    "react-dropzone": "~3.5.3",
    "react-infinite": "~0.9.2",
    "react-redux": "~4.4.5",
    "react-router": "~2.5.2",
    "react-router-redux": "~4.0.5",
    "react-select": "~1.0.0-beta13",
    "react-sortablejs": "~1.0.1",
    "react-virtualized": "~7.12.1",
    "redux": "~3.5.2",
    "serialport": "~4.0.1",
    "serve-favicon": "~2.3.0",
    "serve-static": "~1.11.1",
    "session-file-store": "~0.2.0",
    "sha1": "~1.1.1",
    "socket.io": "~1.4.8",
    "sortablejs": "~1.4.2",
    "stacktrace-js": "~1.3.0",
    "superagent": "~2.0.0",
    "three": "~0.78.0",
    "webappengine": "~1.1.2",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "babel-cli": "~6.10.1",
    "babel-core": "~6.10.4",
    "babel-eslint": "~6.1.2",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-0": "~6.5.0",
    "coveralls": "~2.11.11",
    "css-loader": "~0.23.1",
    "electron-builder": "~5.12.1",
    "electron-packager": "~7.3.0",
    "electron-prebuilt": "~1.2.7",
    "electron-rebuild": "~1.1.5",
    "eslint": "~2.13.1",
    "eslint-config-airbnb": "~9.0.1",
    "eslint-loader": "~1.4.1",
    "eslint-plugin-import": "~1.10.2",
    "eslint-plugin-jsx-a11y": "~1.5.3",
    "eslint-plugin-react": "~5.2.2",
    "esprima": "~2.7.2",
    "eventsource-polyfill": "~0.9.6",
    "file-loader": "~0.9.0",
    "find-imports": "~0.5.0",
    "github": "~2.2.0",
    "glob": "~7.0.3",
    "gulp": "~3.9.1",
    "gulp-babel": "~6.1.2",
    "gulp-sort": "~2.0.0",
    "gulp-util": "~3.0.7",
    "i18next-scanner": "~1.5.0",
    "json-loader": "~0.5.4",
    "nib": "~1.1.0",
    "redux-devtools": "~3.3.1",
    "redux-devtools-chart-monitor": "~1.4.1",
    "redux-devtools-diff-monitor": "~5.0.5",
    "redux-devtools-dock-monitor": "~1.1.1",
    "redux-devtools-log-monitor": "~1.0.11",
    "require-dir": "~0.3.0",
    "run-sequence": "~1.2.2",
    "style-loader": "~0.13.1",
    "stylint": "~1.5.6",
    "stylint-loader": "~1.0.0",
    "stylus": "~0.54.5",
    "stylus-loader": "~2.1.0",
    "tap": "~6.1.1",
    "text-table": "~0.2.0",
    "transform-loader": "~0.2.3",
    "url-loader": "~0.5.7",
    "webpack": "~1.13.1",
    "webpack-dev-middleware": "~1.6.1",
    "webpack-hot-middleware": "~2.12.1"
  }
}
