{
  "name": "cncjs",
  "version": "1.9.0-alpha",
  "description": "A web-based interface for CNC milling controller running Grbl, Smoothieware, or TinyG",
  "homepage": "https://github.com/cncjs/cncjs",
  "author": "Cheton Wu <cheton@gmail.com>",
  "main": "./dist/cnc/cnc.js",
  "bin": {
    "cnc": "./bin/cnc"
  },
  "files": [
    "bin",
    "dist",
    "static"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:cncjs/cncjs.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.x"
  },
  "preferGlobal": true,
  "scripts": {
    "package-update": "babel-node package-update",
    "prepublish": "npm run clean && npm run lint && npm run build",
    "prebuild": "npm run prebuild-prod",
    "prebuild-latest": "bash ./scripts/prebuild-latest.sh",
    "prebuild-dev": "bash ./scripts/prebuild-dev.sh",
    "prebuild-prod": "bash ./scripts/prebuild-prod.sh",
    "build": "gulp prod",
    "build-latest": "gulp prod",
    "build-dev": "gulp dev",
    "build-prod": "gulp prod",
    "clean": "bash -c 'rm -rf ./dist ./output'",
    "electron": "electron",
    "electron-builder": "build",
    "electron-packager": "electron-packager",
    "electron-rebuild": "electron-rebuild",
    "github-release": "github-release",
    "build:mac-x64": "bash -c 'scripts/electron-builder.sh --mac --x64'",
    "build:linux-ia32": "bash -c 'scripts/electron-builder.sh --linux --ia32'",
    "build:linux-x64": "bash -c 'scripts/electron-builder.sh --linux --x64'",
    "build:linux-armv7l": "bash -c 'scripts/electron-builder.sh --linux --armv7l'",
    "build:win-ia32": "bash -c 'scripts/electron-builder.sh --win --ia32'",
    "build:win-x64": "bash -c 'scripts/electron-builder.sh --win --x64'",
    "start": "./bin/cnc",
    "start-electron": "electron ./dist/cnc/main",
    "dev": "npm run build-dev && NODE_ENV=development ./bin/cnc -vv -p 8000 --watch-directory ../../examples/gcode",
    "prod": "npm run build-prod && NODE_ENV=production ./bin/cnc",
    "lint": "npm run eslint && npm run stylint",
    "eslint": "eslint --ext .js --ext .jsx *.js src bin gulp test",
    "stylint": "stylint src/web",
    "test": "node babel-tap --coverage test/*.js",
    "coveralls": "node babel-tap --coverage --coverage-report=text-lcov test/*.js | node_modules/.bin/coveralls"
  },
  "build": {
    "appId": "com.github.cncjs",
    "asar": false,
    "asarUnpack": [],
    "productName": "cncjs",
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "dmg": {
      "background": "build/background.png",
      "icon": "build/icon.icns",
      "iconSize": 80,
      "iconTextSize": 12,
      "contents": [
        {
          "x": 448,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 344,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "deb"
    }
  },
  "directories": {
    "buildResources": "build",
    "output": "output",
    "app": "dist/cnc"
  },
  "keywords": [
    "cncjs",
    "arduino",
    "raspberry",
    "pi",
    "cnc",
    "gcode",
    "grbl",
    "smoothie",
    "smoothieware",
    "tinyg",
    "g2core",
    "react",
    "socket.io"
  ],
  "dependencies": {
    "@trendmicro/react-anchor": "~0.5.2",
    "@trendmicro/react-breadcrumbs": "~0.5.0",
    "@trendmicro/react-interpolate": "~0.5.1",
    "@trendmicro/react-modal": "~0.6.0",
    "@trendmicro/react-paginations": "~0.5.1",
    "async": "~2.1.4",
    "babel-runtime": "~6.22.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.16.0",
    "bootstrap": "~3.3.7",
    "chalk": "~1.1.3",
    "classnames": "~2.2.5",
    "colornames": "~1.1.1",
    "commander": "~2.9.0",
    "compression": "~1.6.2",
    "connect-multiparty": "~2.0.0",
    "connect-restreamer": "~1.0.3",
    "consolidate": "~0.14.5",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "del": "~2.2.2",
    "delay": "~1.3.1",
    "errorhandler": "~1.5.0",
    "es5-shim": "~4.5.9",
    "express": "~4.14.1",
    "express-jwt": "~5.1.0",
    "express-session": "~1.15.0",
    "font-awesome": "~4.7.0",
    "fs-extra": "~1.0.0",
    "gcode-interpreter": "~1.1.1",
    "gcode-parser": "~1.1.1",
    "gcode-toolpath": "~1.1.1",
    "history": "~3.2.1",
    "hogan.js": "~3.0.2",
    "i18next": "~6.0.3",
    "i18next-browser-languagedetector": "~1.0.1",
    "i18next-express-middleware": "~1.0.2",
    "i18next-node-fs-backend": "~0.1.3",
    "i18next-xhr-backend": "~1.3.0",
    "infinite-tree": "~1.7.3",
    "is-electron": "~2.0.0",
    "js-polyfills": "~0.1.30",
    "jsonwebtoken": "~7.2.1",
    "jsuri": "~1.3.1",
    "lodash": "~4.17.4",
    "method-override": "~2.3.7",
    "minimatch": "~3.0.3",
    "moment": "~2.17.1",
    "morgan": "~1.7.0",
    "mousetrap": "~1.6.0",
    "namespace-constants": "~0.5.0",
    "normalize.css": "~5.0.0",
    "package-json": "~2.4.0",
    "parse-json": "~2.2.0",
    "pubsub-js": "~1.5.4",
    "range_check": "~1.4.0",
    "rc-slider": "~6.0.1",
    "rc-table": "~5.2.8",
    "react": "~15.4.2",
    "react-addons-shallow-compare": "~15.4.2",
    "react-addons-update": "~15.4.2",
    "react-bootstrap": "~0.30.7",
    "react-css-modules": "~4.1.0",
    "react-dom": "~15.4.2",
    "react-dropzone": "~3.9.2",
    "react-facebook-loading": "~0.6.0",
    "react-infinite": "~0.10.0",
    "react-infinite-tree": "~0.5.2",
    "react-modal": "~1.6.5",
    "react-redux": "~5.0.2",
    "react-router": "~3.0.0",
    "react-router-redux": "~4.0.7",
    "react-select": "~1.0.0-rc.3",
    "react-sortablejs": "~1.3.0",
    "react-toggle": "~2.2.1",
    "react-validation": "~2.10.9",
    "react-virtualized": "~8.11.4",
    "react-webcam": "0.0.14",
    "redux": "~3.6.0",
    "semver": "~5.3.0",
    "serialport": "~4.0.7",
    "serve-favicon": "~2.3.2",
    "serve-static": "~1.11.1",
    "session-file-store": "~1.0.0",
    "sha1": "~1.1.1",
    "shortid": "~2.2.6",
    "socket.io": "~1.7.2",
    "socket.io-client": "~1.7.2",
    "socketio-jwt": "~4.5.0",
    "sortablejs": "~1.5.0-rc1",
    "spawn-default-shell": "~2.0.0",
    "stacktrace-js": "~1.3.1",
    "superagent": "~3.4.1",
    "superagent-use": "~0.1.0",
    "three": "~0.84.0",
    "uuid": "~3.0.1",
    "watch": "~1.0.1",
    "webappengine": "~1.1.3",
    "winston": "~2.3.0"
  },
  "devDependencies": {
    "babel-cli": "~6.22.2",
    "babel-core": "~6.22.1",
    "babel-eslint": "~7.1.1",
    "babel-loader": "~6.2.10",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-plugin-transform-proto-to-assign": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.22.0",
    "babel-polyfill": "~6.22.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-react": "~6.22.0",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-0": "~6.22.0",
    "bundle-loader": "~0.5.4",
    "coveralls": "~2.11.15",
    "css-loader": "~0.26.1",
    "css-split-webpack-plugin": "~0.2.3",
    "electron": "~1.4.14",
    "electron-builder": "~12.2.0",
    "electron-rebuild": "~1.5.6",
    "eslint": "~3.14.1",
    "eslint-config-trendmicro": "~0.5.1",
